        <div class="container">

                        <a href="/RegistrarCompra" class="btn btn-outline-dark">Registrar Compra Nueva</a>
                        <h1 class="text-center">Listado Compras</h1>
                        <br><br>

                    <div class="container-table">
                        <table class="table ">
                            <th>ID Compra</th>
                            <th>Fecha</th>
                            <th>Cantidad Producto</th>
                            <th>Valor Total</th>
                            <th>Descripción</th>
                            <th>Nombre Cliente</th>
                            <th>nombre Producto</th>
                            <th>Eliminar Compra</th>
                            <tbody>
                                <% for(var i=0;i<listadoCompra.length;i++){ %> 
                                    <tr>
                                        <td><%=listadoCompra[i].PK_idCompra %></td>
                                        <td><%=listadoCompra[i].fechaCompra%></td>
                                        <td><%=listadoCompra[i].cantidadProducto%></td>
                                        <td><%=listadoCompra[i].valorTotalProducto%></td>
                                        <td><%=listadoCompra[i].descripcion_compra%></td>
                                        <td><%=listadoCompra[i].nombreCliente%></td>
                                        <td><%=listadoCompra[i].nombreProducto%></td>
                                        <td> <a href="/EliminarCompra/<%=listadoCompra[i].PK_id_cliente%>"><i class="fa-regular fa-trash-can"></i></a></td>
                                    </tr>
                                <% } %>  
                            </tbody>
                        </table>
                    </div>


        </div>
